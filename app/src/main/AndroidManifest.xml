<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zw.co.researchhub.happyfirst">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".User.ManageStudentActivity"></activity>
        <activity android:name=".Welcome">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale"
                    android:screenOrientation="portrait" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".GuestActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".GeneralTip.ReadActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".LoggedInActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AssessmentActivity"
            android:screenOrientation="portrait"></activity>
 <activity
            android:name=".User.ManageChildrenActivity"
            android:screenOrientation="portrait"></activity>


    </application>

</manifest>